extends layout

block content
  div(id="everything", class="row")
    div(id="editor", class="span6")
      p #{etherpadId}
      iframe(src="http://piratepad.be/p/" + etherpadId + "?showChat=false" height="500px" width="460px" data-etherpad-id=etherpadId)
    div(id="render", class="span6")
      img(id='graph' style='max-height:80vh')
  div(class="row")
    pre(id="msg", class="alert alert-block")
